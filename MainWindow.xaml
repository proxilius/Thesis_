                    </Grid>
                </Grid>
            </TabItem>

            <TabItem Header="Értékpapírszámlák">

                <Grid Margin="0,10,10,10">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="2*"/>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>
                    <Grid Grid.Column="0" Grid.Row="0" Margin="0,0,0,20">
                        <TextBlock FontWeight="Bold" FontSize="20" Margin="419,0,291,298" TextDecorations="{x:Null}" HorizontalAlignment="Center"><Run Text="Értékpapírszámla"/></TextBlock>
                        <Label x:Name="labeldarab" Content="Darabszám" Margin="654,476,155,-176" RenderTransformOrigin="0.5,0.5">
                            <Label.RenderTransform>
                                <TransformGroup>
                                    <ScaleTransform/>
                                    <SkewTransform AngleX="-0.44"/>
                                    <RotateTransform/>
                                    <TranslateTransform X="-0.492"/>
                                </TransformGroup>
                            </Label.RenderTransform>
                        </Label>
                    </Grid>
                    <Grid Grid.Row="1" Margin="0,0,10,0">
                        <DataGrid Margin="767,54,-449,26" x:Name="CommercialPapersDataGrid" AutoGenerateColumns="False" >
                            <DataGrid.Columns>
                                <DataGridTextColumn Binding="{Binding cp_id}" ClipboardContentBinding="{x:Null}" Foreground="#FFA04747" Width="0.5*" Header="ID"/>
                                <DataGridTextColumn Binding="{Binding cp_name}" ClipboardContentBinding="{x:Null}" Foreground="#FFA04747" Width="1.2*" Header="Értékpapír neve"/>
                                <DataGridTextColumn Binding="{Binding cp_date}" ClipboardContentBinding="{x:Null}" Foreground="#FFA04747" Width="*" Header="Vásárlás dátuma" FrameworkElement.FlowDirection="RightToLeft"/>
                                <DataGridTextColumn Binding="{Binding cp_time}" ClipboardContentBinding="{x:Null}" Foreground="#FFA04747" Width="*" Header="Időpontja"/>
                                <DataGridTextColumn Binding="{Binding cp_value, StringFormat=\{0:N0\}}" ClipboardContentBinding="{x:Null}" Foreground="#FFA04747" Width="2*" Header="Vásárlás dátumán összeg"/>
                                <DataGridTextColumn Binding="{Binding cp_amount}" ClipboardContentBinding="{x:Null}" Foreground="#FFA04747" Width="*" Header="Darabszám"/>
                                <DataGridTextColumn Binding="{Binding sumcom, StringFormat=\{0:N0\}}" ClipboardContentBinding="{x:Null}" Foreground="#FFA04747" Width="*" Header="Összesen"/>

                            </DataGrid.Columns>

                        </DataGrid>
                        <DataGrid x:Name="dataGridCommercialsells" Margin="0,54,295,26" AutoGenerateColumns="False">
                            <DataGrid.Columns>
                                <DataGridTextColumn Binding="{Binding Id}" ClipboardContentBinding="{x:Null}" Foreground="#FFA04747" Width="0.5*" Header="ID"/>
                                <DataGridTextColumn Binding="{Binding cpName}" ClipboardContentBinding="{x:Null}" Foreground="#FFA04747" Width="1.4*" Header="Értékpapír neve"/>
                                <DataGridTextColumn Binding="{Binding cpDate}" ClipboardContentBinding="{x:Null}" Foreground="#FFA04747" Width="1.4*" Header="Eladás dátuma" FrameworkElement.FlowDirection="RightToLeft"/>
                                <DataGridTextColumn Binding="{Binding cpTime}" ClipboardContentBinding="{x:Null}" Foreground="#FFA04747" Width="*" Header="Időpontja"/>
                                <DataGridTextColumn Binding="{Binding cpValue, StringFormat=\{0:N0\}}" ClipboardContentBinding="{x:Null}" Foreground="#FFA04747" Width="*" Header="Értéke"/>
                                <DataGridTextColumn Binding="{Binding cpAmount}" ClipboardContentBinding="{x:Null}" Foreground="#FFA04747" Width="*" Header="Darabszám"/>
                                <DataGridTextColumn Binding="{Binding cpSumcom, StringFormat=\{0:N0\}}" ClipboardContentBinding="{x:Null}" Foreground="#FFA04747" Width="*" Header="Összesen"/>
                                <DataGridTextColumn Binding="{Binding cpWinning, StringFormat=\{0:N0\}}" ClipboardContentBinding="{x:Null}" Foreground="#FFA04747" Width="*" Header="Hozam"/>

                            </DataGrid.Columns>
                        </DataGrid>
                        <Label x:Name="label7" Content="Vételek" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="15,-312,0,0" FontWeight="Bold" FontSize="16"/>
                    </Grid>
                    <Grid Grid.Row="1" Grid.Column="1">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="14*"/>
                            <ColumnDefinition Width="25*"/>
                        </Grid.ColumnDefinitions>
                        <Button Content="Új értékpapír vásárlás" Margin="-288,-272,294,570" Click="addCommercialPaper"/>
                        <TextBox x:Name="amountBox" Margin="-290,126,395,194"/>
                        <ComboBox x:Name="fifoname" Margin="-300,200,395,114">
                            <ComboBoxItem IsSelected="True" Content="OTP"/>
                            <ComboBoxItem Content="MOL" />
                            <ComboBoxItem Content="SHELL"/>
                            <ComboBoxItem Content="ERSTE"/>
                        </ComboBox>
                        <TextBox x:Name="fifoamount" Margin="-300,246,426,68"/>
                        <Button Content="LIFO eladás" Margin="-231,233,321,81" Click="sellLifo"/>
                        <Button Content="FIFO eladás" Margin="-231,200,321,114" Click="sellFifo"/>
                        <Label x:Name="labeldarab_Copy1" Content="Db" Margin="-264,246,395,68"/>
                    </Grid>
                    <Button Content="Kiválasztott &#xA;értékpapír eladás" Margin="592,68,149,233" Click="sellCommercialPaper" RenderTransformOrigin="0.278,0.48" Grid.Row="1"/>

