                        </chartingToolkit:Chart>
                        <ComboBox x:Name="comboBoxcpname" HorizontalAlignment="Left" VerticalAlignment="Top" Width="120" Margin="0,19,0,0">
                            <ComboBoxItem IsSelected="True">OTP</ComboBoxItem>
                            <ComboBoxItem >MOL</ComboBoxItem>
                            <ComboBoxItem>SHELL</ComboBoxItem>
                            <ComboBoxItem>ERSTE</ComboBoxItem>
                        </ComboBox>
                        <Button x:Name="button3" Content="Frissít" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="147,21,0,0" Click="GrafikonDarabszam"/>
                    </Grid>
                </ScrollViewer>

            </TabItem>
            <TabItem Header="Részvény érték grafikon">
                <ScrollViewer  HorizontalScrollBarVisibility="Visible" VerticalScrollBarVisibility="Visible" Margin="0,0,0,99">

                    <Grid HorizontalAlignment="Left"  VerticalAlignment="Top" >
                        <chartingToolkit:Chart Name="lineChartForPaperValue"
                                       Title="Részvény érték grafikon" 
                                       VerticalAlignment="Top" 
                                       Margin="0,68,10,0" 
                                       MinHeight="1500" MinWidth="2500">
                            <chartingToolkit:LineSeries Name="KWGsd"
                                                Title="Valasztott"  
                                                DependentValuePath="Value" 
                                                IndependentValuePath="Key"
                                                ItemsSource="{Binding [0]}"
                                                IsSelectionEnabled="True" Margin="0,0,1,0"/>
                        </chartingToolkit:Chart>
                        <ComboBox x:Name="comboBox2" HorizontalAlignment="Left" VerticalAlignment="Top" Width="120">
                            <ComboBoxItem IsSelected="True">OTP</ComboBoxItem>
                            <ComboBoxItem >MOL</ComboBoxItem>
                            <ComboBoxItem>SHELL</ComboBoxItem>
                            <ComboBoxItem>ERSTE</ComboBoxItem>
                        </ComboBox>
                        <Button x:Name="button4" Content="Frissít" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="138,2,0,0"  Click="GrafikonReszvenyErtek"/>


                    </Grid>
                </ScrollViewer>


            </TabItem>


        </TabControl>
    </Grid>




</Window>
