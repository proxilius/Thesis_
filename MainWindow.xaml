        xmlns:System="clr-namespace:System;assembly=mscorlib" x:Class="SimaSzamlaAdatbazissal.MainWindow"

        mc:Ignorable="d"
        Title="MainWindow" Height="724" Width="1348.575">


    <Grid Margin="0,0,0,22">
        <TabControl x:Name="tabControl" VerticalAlignment="Top" Height="743" Margin="0,0,0,-72"  >

            <TabItem Header="Számlák">
                <Grid>
                    <Grid Margin="10,0,0,0">

                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="123*"/>
                            <ColumnDefinition Width="84*"/>
                            <ColumnDefinition Width="30*"/>
                            <ColumnDefinition Width="385*"/>
                            <ColumnDefinition Width="80*"/>
                            <ColumnDefinition Width="150*"/>
                        </Grid.ColumnDefinitions>
                        <Grid Grid.Column="0" Margin="-10,10,10,-10" Grid.ColumnSpan="2" >
