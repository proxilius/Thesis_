                        <DataGrid.Columns>
                            <DataGridTextColumn Binding="{Binding cpName}" ClipboardContentBinding="{x:Null}" Foreground="#FFA04747" Width="*" Header="Neve"/>
                            <DataGridTextColumn Binding="{Binding cpDate}" ClipboardContentBinding="{x:Null}" Foreground="#FFA04747" Width="2*" Header="Dátum"/>
                            <DataGridTextColumn Binding="{Binding cpDB}" ClipboardContentBinding="{x:Null}" Foreground="#FFA04747" Width="*" Header="Darabszám"/>
                            <DataGridTextColumn Binding="{Binding cpFulValue}" ClipboardContentBinding="{x:Null}" Foreground="#FFA04747" Width="*" Header="Összérték"/>

                        </DataGrid.Columns>

                    </DataGrid>
                    <Button x:Name="buttonToGetActualAmounts" Content="Frissit" Grid.Column="1" HorizontalAlignment="Left" Margin="227,78,0,0" VerticalAlignment="Top" Width="75" Click="getActualAmount"/>
                    <Label x:Name="label2" Content="Adja meg a dátumot" Grid.Column="1" HorizontalAlignment="Left" Margin="232,10,0,0" VerticalAlignment="Top" Width="143" FontWeight="Bold" FontSize="13"/>
                    <local:ViewModelUserControl Grid.Column="1" HorizontalAlignment="Left" Height="42" Margin="195,208,0,0" VerticalAlignment="Top" Width="100"/>
                    <DatePicker x:Name="datum" Grid.Column="1" HorizontalAlignment="Left" Margin="227,44,0,0" VerticalAlignment="Top" Height="29" Width="116"/>
                    <Label x:Name="label8" Content="Eladások" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,20,0,0" Grid.Row="1" FontWeight="Bold" FontSize="16"/>
                    <Label x:Name="label9" Content="Aktuális darabszám táblázat" HorizontalAlignment="Left" VerticalAlignment="Top" Grid.Column="1" Margin="10,7,0,0" FontSize="16" FontWeight="Bold"/>
                    <DataGrid x:Name="dataGridCommercialFix" HorizontalAlignment="Left" VerticalAlignment="Top" Height="260" Width="536" Margin="18,70,0,0" AutoGenerateColumns="False">
                        <DataGrid.Columns>
                            <DataGridTextColumn Binding="{Binding Id}" ClipboardContentBinding="{x:Null}" Foreground="#FFA04747" Width="0.5*" Header="ID"/>
                            <DataGridTextColumn Binding="{Binding cp_name}" ClipboardContentBinding="{x:Null}" Foreground="#FFA04747" Width="1.2*" Header="Értékpapír neve"/>
                            <DataGridTextColumn Binding="{Binding cp_date}" ClipboardContentBinding="{x:Null}" Foreground="#FFA04747" Width="1.3*" Header="Vásárlás dátuma" FrameworkElement.FlowDirection="RightToLeft"/>
                            <DataGridTextColumn Binding="{Binding cp_time}" ClipboardContentBinding="{x:Null}" Foreground="#FFA04747" Width="*" Header="Időpontja"/>
                            <DataGridTextColumn Binding="{Binding cp_value, StringFormat=\{0:N0\}}" ClipboardContentBinding="{x:Null}" Foreground="#FFA04747" Width="*" Header="Értéke"/>
                            <DataGridTextColumn Binding="{Binding cp_amount}" ClipboardContentBinding="{x:Null}" Foreground="#FFA04747" Width="*" Header="Darabszám"/>
                            <DataGridTextColumn Binding="{Binding sumcom, StringFormat=\{0:N0\}}" ClipboardContentBinding="{x:Null}" Foreground="#FFA04747" Width="*" Header="Összesen"/>

                        </DataGrid.Columns>
                    </DataGrid>
                    <DataGrid x:Name="dataGridActual" HorizontalAlignment="Left" VerticalAlignment="Top" Height="186" Width="507" Grid.ColumnSpan="2" Margin="782,184,0,0" AutoGenerateColumns="False" Grid.RowSpan="2">
                        <DataGrid.Columns>
                            <DataGridTextColumn Binding="{Binding Id}" ClipboardContentBinding="{x:Null}" Foreground="#FFA04747" Width="0.5*" Header="ID"/>
                            <DataGridTextColumn Binding="{Binding Name}" ClipboardContentBinding="{x:Null}" Foreground="#FFA04747" Width="1.2*" Header="Név"/>
                            <DataGridTextColumn Binding="{Binding DateOf}" ClipboardContentBinding="{x:Null}" Foreground="#FFA04747" Width="*" Header="Dátum" FrameworkElement.FlowDirection="RightToLeft"/>
                            <DataGridTextColumn Binding="{Binding TimeOf}" ClipboardContentBinding="{x:Null}" Foreground="#FFA04747" Width="*" Header="Időpont"/>
                            <DataGridTextColumn Binding="{Binding Change}" ClipboardContentBinding="{x:Null}" Foreground="#FFA04747" Width="*" Header="Változás"/>
                            <DataGridTextColumn Binding="{Binding AmountAfterChange, StringFormat=\{0:N0\}}" ClipboardContentBinding="{x:Null}" Foreground="#FFA04747" Width="*" Header="Változás utáni érték"/>
                            <DataGridTextColumn Binding="{Binding Sum, StringFormat=\{0:N0\}}" ClipboardContentBinding="{x:Null}" Foreground="#FFA04747" Width="*" Header="Öszesen"/>
                            <DataGridTextColumn Binding="{Binding actualRate, StringFormat=\{0:N0\}}" ClipboardContentBinding="{x:Null}" Foreground="#FFA04747" Width="*" Header="Árfolyam"/>
                        </DataGrid.Columns>
                    </DataGrid>
                    <Label x:Name="label9_Copy" Content="Aktuális darabszám táblázat lebontva" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="782,148,0,0" FontSize="16" FontWeight="Bold" Grid.ColumnSpan="2"/>
                    <Label x:Name="label8_Copy" Content="Aktuális batch" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="766,25,0,0" Grid.Row="1" FontWeight="Bold" FontSize="16"/>
                </Grid>

            </TabItem>
            <TabItem Header="Havi kimutatás">
                <Grid HorizontalAlignment="Left" Height="709" VerticalAlignment="Top" Width="1329">
                    <DatePicker Name="BeginDate" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,26,0,0"/>
                    <DatePicker Name="EndDate" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="160,26,0,0"/>
                    <Label x:Name="label11" Content="Kezdő dátum" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,0,0,0"/>
                    <Label x:Name="label12" Content="Végső dátum" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="160,0,0,0"/>
                    <DataGrid x:Name="dataGridFilterByDate" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,71,0,0" Width="453" Height="220"/>
                    <Button x:Name="button2" Content="Listáz" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="388,30,0,0" Click="ListPapersByDate"/>
                </Grid>
            </TabItem>
            <TabItem Header="Árfolyam">
                <Grid HorizontalAlignment="Left" Height="547" VerticalAlignment="Top" Width="1328">
                    <Label x:Name="label1" Content="Árfolyam rögzítése" HorizontalAlignment="Left" VerticalAlignment="Top" FontWeight="Bold" FontSize="14"/>
                    <ComboBox x:Name="comboBox" HorizontalAlignment="Left" VerticalAlignment="Top" Width="120" Margin="0,49,0,0">
                        <ComboBoxItem IsSelected="True">OTP</ComboBoxItem>
                        <ComboBoxItem >MOL</ComboBoxItem>
                        <ComboBoxItem>SHELL</ComboBoxItem>
                        <ComboBoxItem>ERSTE</ComboBoxItem>
                    </ComboBox>
                    <Label x:Name="label3" Content="Értékpapír neve:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,23,0,0"/>
                    <DatePicker Name="getDate" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,76,0,0"/>
                    <TextBox x:Name="timeOf" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="00:00:00" VerticalAlignment="Top" Width="120" Margin="0,105,0,0"/>
                    <Label x:Name="label4" Content="Értékpapír árfolyama a kiválasztott időpontban:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,133,0,0"/>
                    <TextBox x:Name="priceOfTable" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" Margin="0,164,0,0"/>
                    <Label x:Name="label5" Content="Ft" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="125,164,0,0"/>
                    <Label x:Name="label6" Content="Időpont megadása" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="125,105,0,0"/>
                    <Button x:Name="buttonAddToRateTable" Content="Rögzít" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="0,202,0,0" Click="addPriceToDatabase"/>
                    <DataGrid x:Name="dataGridRate" HorizontalAlignment="Left" VerticalAlignment="Top" Height="149" Width="864" Margin="325,49,0,0"/>
                    <chartingToolkit:Chart Name="lineChart"
                                       Title="Árfolyam grafikon" 
                                       VerticalAlignment="Top" 
                                       Margin="10,242,10,-128" 
                                       Height="433">
                        <chartingToolkit:LineSeries Name="KWG"
                                                Title="OTP"  
                                                DependentValuePath="Value" 
                                                IndependentValuePath="Key"
                                                ItemsSource="{Binding [0]}"
                                                IsSelectionEnabled="True"/>
                        <chartingToolkit:LineSeries Name="Mol"
                                                Title="MOL"  
                                                DependentValuePath="Value" 
                                                IndependentValuePath="Key"
                                                ItemsSource="{Binding [1]}"
                                                IsSelectionEnabled="True"/>
                        <chartingToolkit:LineSeries Name="Erste"
                                                Title="Kiválasztott"  
                                                DependentValuePath="Value" 
                                                IndependentValuePath="Key"
                                                ItemsSource="{Binding [2]}"
                                                IsSelectionEnabled="True"/>

                    </chartingToolkit:Chart>
                    <Button x:Name="buttonDeletePrice" Content="Töröl" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="89,202,0,0" Click="deletePrice"/>
                    <Label x:Name="label10" Content="Árfolyam táblázat" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="325,11,0,0" FontWeight="Bold" FontSize="14"/>
                    <ComboBox x:Name="comboBox1" HorizontalAlignment="Left" VerticalAlignment="Top" Width="120" Margin="183,202,0,0">
                        <ComboBoxItem IsSelected="True">OTP</ComboBoxItem>
                        <ComboBoxItem >MOL</ComboBoxItem>
                        <ComboBoxItem>SHELL</ComboBoxItem>
                        <ComboBoxItem>ERSTE</ComboBoxItem>
                    </ComboBox>
                    <Button x:Name="buttonmakediagram" Content="Frissít" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="325,202,0,0" Click="makeDiagram"/>
                    <chartingToolkit:Chart x:Name="lineChartCopy"
                        Title="Árfolyam grafikon" 
                        VerticalAlignment="Top" 
                        Margin="10,242,10,-128" 
                        Height="433" d:IsHidden="True">
                        <chartingToolkit:LineSeries x:Name="Kivalasztottak"
                            Title="Kiválasztott"  
                            DependentValuePath="Value" 
                            IndependentValuePath="Key"
                            ItemsSource="{Binding [0]}"
                            IsSelectionEnabled="True"/>
                    </chartingToolkit:Chart>

                </Grid>



            </TabItem>
            <TabItem Header="Részvény grafikon">
                <ScrollViewer  HorizontalScrollBarVisibility="Visible" VerticalScrollBarVisibility="Visible" Margin="0,0,0,99">
                    <Grid HorizontalAlignment="Left"  VerticalAlignment="Top" Width="2700"  >
                        <chartingToolkit:Chart Name="lineChartForPaperDB"
                                       Title="Részvény darabszám grafikon" 
