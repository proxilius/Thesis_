                            <DataGrid.Columns>
                                <DataGridTextColumn Binding="{Binding Id}" ClipboardContentBinding="{x:Null}" Foreground="#FFA04747" Width="*" Header="ID"/>
                                <DataGridTextColumn Binding="{Binding Megnevezes}" ClipboardContentBinding="{x:Null}" Foreground="#FFA04747" Width="*" Header="Név"/>
                                <DataGridTextColumn Binding="{Binding Osszeg, StringFormat=\{0:N0\}}" ClipboardContentBinding="{x:Null}" Foreground="#FFA04747" Width="*" Header="Összeg" FontWeight="Bold">
                                    <DataGridTextColumn.ElementStyle>
                                        <Style>
                                            <Setter Property="TextBlock.TextAlignment" Value="Right" />
                                        </Style>
                                    </DataGridTextColumn.ElementStyle>
                                </DataGridTextColumn>

                                <DataGridTextColumn Binding="{Binding Datum}" ClipboardContentBinding="{x:Null}" Foreground="#FFA04747" Width="*" Header="Dátum"/>

                            </DataGrid.Columns>

                        </DataGrid>
                        <DataGrid Grid.Column="4" x:Name="ReszosszegDatagrid" HorizontalAlignment="Left" AutoGenerateColumns="False" Margin="5,10,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.667,1.231" Grid.ColumnSpan="1" Width="78" Height="398" >
                            <DataGrid.Columns>
                                <DataGridTextColumn Binding="{Binding sub, StringFormat=\{0:N0\}}"  ClipboardContentBinding="{x:Null}" Foreground="#FFA04747" Width="*" Header="Részösszeg"/>
                            </DataGrid.Columns>
                        </DataGrid>
                        <Grid Grid.Column="5">
                            <TextBlock x:Name="textBlock1" HorizontalAlignment="Left" Margin="28,0,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="126"><Run FontWeight="Bold" FontSize="16" FontFamily="Times New Roman" Text="Exportálás excel munkafüzetbe"/><LineBreak/><Run/></TextBlock>
