                                       Margin="0,68,10,0" 
                                       MinHeight="500" MinWidth="2700">
                            <chartingToolkit:LineSeries Name="KWGs"
                                                Title="OTP"  
                                                DependentValuePath="Value" 
                                                IndependentValuePath="Key"
                                                ItemsSource="{Binding [0]}"
